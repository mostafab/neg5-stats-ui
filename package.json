{
  "name": "neg5.stats.web-server",
  "version": "1.0.0",
  "description": "Server for neg 5 stats web",
  "engines": {
    "node": "16.18.1"
  },
  "scripts": {
    "start": "npm run build && npm run serve",
    "build": "babel server/ -d server-dist",
    "serve": "node server-dist/",
    "heroku-postbuild": "next build",
    "start-dev": "nodemon --watch server --exec babel-node server/"
  },
  "cacheDirectories": [
    "node_modules",
    ".next/cache"
  ],
  "dependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.20.5",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/register": "7.18.9",
    "axios": "1.2.1",
    "bootswatch": "3.3.7",
    "dotenv": "4.0.0",
    "express": "4.18.1",
    "express-http-proxy": "1.6.3",
    "express-sslify": "1.2.0",
    "font-awesome": "4.7.0",
    "lodash": "4.17.11",
    "moment": "2.18.1",
    "morgan": "1.10.0",
    "next": "12.2.0",
    "next-redux-wrapper": "8.0.0",
    "prop-types": "15.6.2",
    "query-string": "5.0.0",
    "react": "17.0.2",
    "react-addons-shallow-compare": "15.6.0",
    "react-bootstrap": "0.31.2",
    "react-bootstrap-typeahead": "2.0.0-rc.4",
    "react-dates": "21.8.0",
    "react-dom": "17.0.2",
    "react-fontawesome": "1.6.1",
    "react-redux": "7.2.5",
    "redux": "3.6.0",
    "redux-actions": "2.2.1",
    "redux-thunk": "2.2.0",
    "regenerator-runtime": "0.13.11",
    "sanitize.css": "5.0.0",
    "slug": "0.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mostafab/neg5.stats.web-server"
  },
  "keywords": [
    "node",
    "heroku",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.20.5",
    "@babel/preset-env": "7.20.2",
    "@babel/register": "7.18.9",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "nodemon": "1.11.0",
    "prettier": "2.8.1"
  }
}
