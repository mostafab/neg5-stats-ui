{
  "name": "neg5.stats.web-server",
  "version": "1.0.0",
  "description": "Server for neg 5 stats web",
  "engines": {
    "node": "8.17.x"
  },
  "scripts": {
    "start": "npm run build && npm run serve",
    "build": "babel server/ -d server-dist",
    "serve": "node server-dist/",
    "heroku-postbuild": "cd react-ui/ && yarn install && yarn install --only=dev --no-shrinkwrap && npm run build",
    "start-dev": "nodemon --watch server --exec babel-node server/"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "@babel/core": "7.20.5",
    "@babel/cli": "7.19.3",
    "@babel/preset-env": "7.20.2",
    "@babel/register": "7.18.9",
    "dotenv": "4.0.0",
    "express": "4.18.1",
    "express-http-proxy": "1.6.3",
    "express-sslify": "1.2.0",
    "morgan": "1.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mostafab/neg5.stats.web-server"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/cli": "7.19.3",
    "@babel/preset-env": "7.20.2",
    "@babel/register": "7.18.9",
    "nodemon": "1.11.0"
  }
}
