{
  "name": "neg5.stats.web-server",
  "version": "1.0.0",
  "description": "Server for neg 5 stats web",
  "engines": {
    "node": "6.11.x"
  },
  "scripts": {
    "start": "npm run build && npm run serve",
    "build": "babel server/ -d server-dist",
    "serve": "node server-dist/",
    "heroku-postbuild": "cd react-ui/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "start-dev": "nodemon --watch server --exec babel-node server/"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "babel": "6.23.0",
    "babel-cli": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.26.0",
    "dotenv": "4.0.0",
    "express": "4.14.1",
    "express-http-proxy": "1.4.0",
    "express-sslify": "1.2.0",
    "morgan": "1.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mostafab/neg5.stats.web-server"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.26.0",
    "nodemon": "1.11.0"
  }
}
